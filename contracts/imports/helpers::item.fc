cell item::get_state_init(cell code, int index, slice authoritry_address) {
    cell data = begin_cell().store_uint(index, 256).store_slice(authoritry_address).end_cell();
    return calculate_state_init(code, data);
}

slice item::get_address(int workchain, cell state_init) {
    return begin_cell().store_uint(4, 3)
            .store_int(workchain, 8)
            .store_uint(cell_hash(state_init), 256)
            .end_cell()
            .begin_parse();
}
